    <fields>
        <fullName>Email_Consent_Expiration_Date__c</fullName>
        <description>This field contains the expiration date for the contact&apos;s email consent statement. The formula adds two years to the Email Consent Updated Date, and takes leap year into account (VSA-9).</description>
        <externalId>false</externalId>
        <formula>IF( 
AND( 
MONTH( Email_Consent_Updated_Date__c ) = 2, 
DAY( Email_Consent_Updated_Date__c ) = 29 
), 
DATE( 
YEAR(Email_Consent_Updated_Date__c )+2, 
3, 
1), 
DATE( 
YEAR(Email_Consent_Updated_Date__c )+2, 
MONTH(Email_Consent_Updated_Date__c ), 
DAY(Email_Consent_Updated_Date__c ) 
) 
)</formula>
        <inlineHelpText>Automatically calculated based on Email Consent Updated Date.</inlineHelpText>
        <label>Email Consent Expiration Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Email_Consent_Purposes__c</fullName>
