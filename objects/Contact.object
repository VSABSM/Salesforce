    <validationRules>
        <fullName>Consent_Must_Have_Purposes</fullName>
        <active>true</active>
        <description>Purposes must be selected when Consent Status is set to &quot;Consent Given&quot;. This validation rule is triggered if Consent Status is set to &quot;Consent Given&quot;, but no Purposes have been selected for that channel (VSA-9).</description>
        <errorConditionFormula>$Setup.Process_Control__c.Contact_Validation__c = TRUE &amp;&amp;
OR( 
( 
ISPICKVAL( Postal_Consent_Status__c , &quot;Consent Given&quot;) &amp;&amp; 
(ISBLANK( Postal_Consent_Purposes__c ))), 
( 
ISPICKVAL( Email_Consent_Status__c , &quot;Consent Given&quot;) &amp;&amp; 
(ISBLANK( Email_Consent_Purposes__c ))), 
( 
ISPICKVAL( Phone_Consent_Status__c , &quot;Consent Given&quot;) &amp;&amp; 
(ISBLANK( Phone_Consent_Purposes__c ))), 
( 
ISPICKVAL( SMS_Consent_Status__c , &quot;Consent Given&quot;) &amp;&amp; 
(ISBLANK( SMS_Consent_Purposes__c )))
)</errorConditionFormula>
        <errorMessage>Please specify the purposes which the contact is consenting to.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Consent_Status_Update_Must_Update_Date</fullName>
        <active>true</active>
        <description>This validation rule is triggered if the Consent Status for any channel is updated but the Consent Updated value for that channel is not updated.</description>
        <errorConditionFormula>$Setup.Process_Control__c.Contact_Validation__c = TRUE &amp;&amp;

OR( 
   ( 
    ISCHANGED ( Postal_Consent_Status__c ) &amp;&amp; 
    NOT( ISCHANGED ( Postal_Consent_Updated_Date__c ))),
   ( 
    ISCHANGED ( Email_Consent_Status__c ) &amp;&amp; 
    NOT( ISCHANGED ( Email_Consent_Updated_Date__c ))),
   ( 
    ISCHANGED ( Phone_Consent_Status__c ) &amp;&amp; 
    NOT( ISCHANGED ( Phone_Consent_Updated_Date__c ))),
   ( 
    ISCHANGED ( SMS_Consent_Status__c ) &amp;&amp; 
    NOT( ISCHANGED ( SMS_Consent_Updated_Date__c )))
)</errorConditionFormula>
        <errorMessage>When Consent Status is updated you must update the Consent Updated Date. Please ensure to update the Consent Updated Date for all relevant channels.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Consent_Updated_Date_Cannot_Be_Future</fullName>
        <active>true</active>
        <description>Consent Updated Date for any channel cannot be a future date. This validation rule is triggered if a future date is inserted into any of the Consent Updated Date field.</description>
        <errorConditionFormula>$Setup.Process_Control__c.Contact_Validation__c = TRUE &amp;&amp; 

OR(
    Postal_Consent_Updated_Date__c &gt; TODAY(),
    Email_Consent_Updated_Date__c &gt; TODAY(),
    Phone_Consent_Updated_Date__c &gt; TODAY(),
    SMS_Consent_Updated_Date__c &gt; TODAY()
  )</errorConditionFormula>
        <errorMessage>A Consent Updated Date cannot be a future date. Please correct to today&apos;s date or a date in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Preferred_Email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( npe01__Preferred_Email__c ,&quot;&quot;),
OR(
LEN(npe01__AlternateEmail__c  )&gt;0,
LEN(npe01__WorkEmail__c )&gt;0,
LEN(npe01__HomeEmail__c  )&gt;0
)
)</errorConditionFormula>
        <errorDisplayField>npe01__Preferred_Email__c</errorDisplayField>
        <errorMessage>Please select a Preferred Email</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Preferred_Phone</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(npe01__PreferredPhone__c ,&quot;&quot;),
OR(
 LEN(MobilePhone)&gt;0,
 LEN( npe01__WorkPhone__c)&gt;0,
 LEN( OtherPhone )&gt;0
)
)</errorConditionFormula>
        <errorDisplayField>npe01__PreferredPhone__c</errorDisplayField>
        <errorMessage>Please select a Preferred Phone</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Consent_Must_Have_No_Purposes</fullName>
        <active>true</active>
        <description>This validation rule is triggered if the Consent Status for any channel is set to &quot;No Consent&quot;, and the Consent Purposes field has a value. If there is no consent for the channel, there should not be any purposes specified for that channel.</description>
        <errorConditionFormula>$Setup.Process_Control__c.Contact_Validation__c &amp;&amp;

OR(
   (
   ISPICKVAL(  Postal_Consent_Status__c , &quot;No Consent&quot;) &amp;&amp;
   NOT(ISBLANK( Postal_Consent_Purposes__c ))),
   (
   ISPICKVAL( Email_Consent_Status__c , &quot;No Consent&quot;) &amp;&amp;
   NOT(ISBLANK( Email_Consent_Purposes__c ))),
   (
   ISPICKVAL( Phone_Consent_Status__c , &quot;No Consent&quot;) &amp;&amp;
   NOT(ISBLANK( Phone_Consent_Purposes__c ))),
   (
   ISPICKVAL( SMS_Consent_Status__c , &quot;No Consent&quot;) &amp;&amp;
   NOT(ISBLANK( SMS_Consent_Purposes__c )))
)</errorConditionFormula>
        <errorMessage>Purposes are not applicable if consent has not been given. Please check all &apos;Consent Status&apos; and &apos;Consent Purposes&apos; combinations, and update if relevant.</errorMessage>
    </validationRules>
