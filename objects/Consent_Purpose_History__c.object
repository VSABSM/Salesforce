    <deploymentStatus>Deployed</deploymentStatus>
    <description>This object stores changes from any of the Consent Purpose fields on the Contact object. The object is necessary because standard Salesforce Field Tracking History does not capture the old and new values for multi-select picklist fields (VSA-6).</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Action__c</fullName>
        <description>This field contains the &apos;Changed From&apos; and &apos;Changed To&apos; values when someone updates any of the Consent Purpose fields on the Contact object. It is automatically populated by a process builder (VSA-6).</description>
        <externalId>false</externalId>
        <label>Action</label>
        <length>255</length>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Changed_By__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field contains the user who updated any of the Consent Purpose fields on the Contact object. It is populated by a process builder (VSA-6).</description>
        <externalId>false</externalId>
        <label>Changed By</label>
        <referenceTo>User</referenceTo>
        <relationshipName>Consent_Purpose_History</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Changed_Date_Time__c</fullName>
        <description>This field contains the timestamp for when the Consent Purpose field was updated (VSA-6).</description>
        <externalId>false</externalId>
        <label>Changed Date/Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Contact__c</fullName>
        <description>This field contains the contact record which the consent purpose history record is related to (VSA-6).</description>
        <externalId>false</externalId>
        <label>Contact</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Consent Purpose History</relationshipLabel>
        <relationshipName>Consent_Purpose_History</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Updated_Field__c</fullName>
        <description>This field contains a reference to which specific Consent Purpose field the change is triggered by (VSA-6).</description>
        <externalId>false</externalId>
        <label>Updated Field</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Postal Consent Purpose</fullName>
                    <default>false</default>
                    <label>Postal Consent Purpose</label>
                </value>
                <value>
                    <fullName>Email Consent Purpose</fullName>
                    <default>false</default>
                    <label>Email Consent Purpose</label>
                </value>
                <value>
                    <fullName>Phone Consent Purpose</fullName>
                    <default>false</default>
                    <label>Phone Consent Purpose</label>
                </value>
                <value>
                    <fullName>SMS Consent Purpose</fullName>
                    <default>false</default>
                    <label>SMS Consent Purpose</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
