<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This Custom Metadata Type allows you to specify which Process Control settings to disable for the duration of a Gift Aid Claim. Note: This does not disable Process Control for any user anywhere in Salesforce beside of the process of submitting a Gift Aid Claim.</description>
    <fields>
        <fullName>gcga__Active__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>CGAND-259: Gift Aid Claim - Process Control during claim process</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>This field determines whether the Gift Aid Claim Specific Process Control is active. If unchecked, the Process Control setting will not be overwritten to be disabled for the Gift Aid Claim process.</inlineHelpText>
        <label>Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>gcga__Namespace__c</fullName>
        <deprecated>false</deprecated>
        <description>CGAND-259: Gift Aid Claim - Process Control during claim process</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>This field specifies whether the Process Control setting is part of local Process Control, Clarity Gift Aid Process Control or NPSP&apos;s Table-Driven Trigger Management.</inlineHelpText>
        <label>Namespace</label>
        <required>true</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Clarity Gift Aid</fullName>
                    <default>false</default>
                    <label>Clarity Gift Aid</label>
                </value>
                <value>
                    <fullName>NPSP</fullName>
                    <default>false</default>
                    <label>NPSP</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>gcga__Process_Control_Setting__c</fullName>
        <deprecated>false</deprecated>
        <description>CGAND-259: Gift Aid Claim - Process Control during claim process</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>This field specifies the Developer Name of the Process Control setting.</inlineHelpText>
        <label>Process Control Setting</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Gift Aid Claim Specific Process Control</label>
    <pluralLabel>Gift Aid Claim Specific Process Control</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
