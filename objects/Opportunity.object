    <fields>
        <fullName>Today_Date_as_Text__c</fullName>
        <description>This field returns TODAY&apos;S DATE in a text format. It is used in a mail merge template (VSA-29).</description>
        <externalId>false</externalId>
        <formula>IF( 
ISBLANK( Today_Date__c ), 
NULL, 
( 
TEXT(DAY( Today_Date__c ))&amp; 
CASE( 
DAY( Today_Date__c ), 
1, &apos;st&apos;, 
2, &apos;nd&apos;, 
3, &apos;rd&apos;, 
21,&apos;st&apos;, 
22,&apos;nd&apos;, 
23,&apos;rd&apos;, 
31,&apos;st&apos;, 
&apos;th&apos; 

)&amp;&apos; &apos;&amp; 
CASE( 
MONTH( Today_Date__c ), 
1, &quot;January&quot;, 
2, &quot;February&quot;, 
3, &quot;March&quot;, 
4, &quot;April&quot;, 
5, &quot;May&quot;, 
6, &quot;June&quot;, 
7, &quot;July&quot;, 
8, &quot;August&quot;, 
9, &quot;September&quot;, 
10, &quot;October&quot;, 
11, &quot;November&quot;, 
&quot;December&quot; 
)&amp;&apos; &apos;&amp; 
TEXT(YEAR( Today_Date__c )) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Today&apos;s Date as Text</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
