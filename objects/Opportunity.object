        <description>Fees Deducted</description>
        <externalId>false</externalId>
        <formula>IF(Gross_Amount__c = 0,0,Gross_Amount__c -  Amount)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field displays the Gross Amount minus the Net Amount to show the fees deducted by the third party.</inlineHelpText>
        <label>Fees Deducted</label>
        <precision>18</precision>
        <required>false</required>
