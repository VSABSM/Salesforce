        <externalId>false</externalId>
        <formula>IF(
  OR(
    ISPICKVAL( Payment_Method__c , &quot;Cash&quot;),
    ISPICKVAL( Payment_Method__c , &quot;Debit/credit card&quot;),
    ISPICKVAL( Payment_Method__c , &quot;Bank Transfer&quot;),
    ISPICKVAL( Payment_Method__c , &quot;Cheque&quot;),
    ISPICKVAL( Payment_Method__c , &quot;Direct Debit&quot;),
    ISPICKVAL( Payment_Method__c , &quot;Standing Order&quot;)
   ),
  TRUE, 
  FALSE
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Payment Method Gift Aid Eligible</label>
