    <fields>
        <fullName>cc_Donation_Gross_Amount__c</fullName>
        <description>This field contains the donation gross amount as supplied by Charity Checkout. The value is used as the starting value to determine the actual donation credit which VSA will receive after Charity Checkout&apos;s various fees have been deducted from the donation. The value is provided via CC&apos;s Zapier trigger, and is used to calculate the Donation Net Amount (VSA-31).</description>
        <externalId>false</externalId>
        <inlineHelpText>The donation amount the donor paid via the Charity Checkout website before fees are subtracted.</inlineHelpText>
        <label>Donation Gross Amount (CC)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
